<h1>Logs</h1>
<table class="table">
  <thead>
    <tr>
      <th>Action</th>
      <th>Run</th>
      <th>Created</th>
    </tr>
  </thead>

  <tbody>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.action %></td>
        <td><%= log.run %></td>
        <td><%= log.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>